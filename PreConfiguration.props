<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- PreConfiguration build properties -->
  <PropertyGroup>
    <INITIAL_KMDF_VERSION_MAJOR>DUMMY_KMDF_VERSION_MAJOR$([System.Guid]::NewGuid())</INITIAL_KMDF_VERSION_MAJOR>
    <KMDF_VERSION_MAJOR>$(INITIAL_KMDF_VERSION_MAJOR)</KMDF_VERSION_MAJOR>
    <INITIAL_KMDF_VERSION_MINOR>DUMMY_KMDF_VERSION_MINOR$([System.Guid]::NewGuid())</INITIAL_KMDF_VERSION_MINOR>
    <KMDF_VERSION_MINOR>$(INITIAL_KMDF_VERSION_MINOR)</KMDF_VERSION_MINOR>
    <INITIAL_UMDF_VERSION_MAJOR>DUMMY_UMDF_VERSION_MAJOR$([System.Guid]::NewGuid())</INITIAL_UMDF_VERSION_MAJOR>
    <UMDF_VERSION_MAJOR>$(INITIAL_UMDF_VERSION_MAJOR)</UMDF_VERSION_MAJOR>
    <INITIAL_UMDF_VERSION_MINOR>DUMMY_UMDF_VERSION_MINOR$([System.Guid]::NewGuid())</INITIAL_UMDF_VERSION_MINOR>
    <UMDF_VERSION_MINOR>$(INITIAL_UMDF_VERSION_MINOR)</UMDF_VERSION_MINOR>
  </PropertyGroup>
</Project>